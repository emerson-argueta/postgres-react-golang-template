digraph achiever{
    labelloc=t
    label="Achiever"
    subgraph cluster_achiever_properties{
        label="properties"
        
        name[shape=rectangle]
        id[shape=rectangle]
        subgraph cluster_goals{
            label=goals
            goal[shape=rectangle]
        }
        
        name->id->goal[style=invis]
